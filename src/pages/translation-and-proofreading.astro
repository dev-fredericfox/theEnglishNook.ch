---
import HomeLayout from '../layouts/HomeLayout.astro';
import MetaSEO from '../components/MetaSEO.astro';
import Translation from '../components/Translation.astro';
import Testimonials from '../components/Testimonials.astro';
import NavBar from '../components/NavBar.astro';
import SideBar from '../components/SideBar.astro';
import Footer from '../components/Footer.astro';
---

<HomeLayout>
    <MetaSEO slot="MetaSEO" lang="en" location="translation"/>
    <NavBar slot="NavBar" lang="en" location="translation"/>
    <SideBar slot="SideBar" lang="en" location="translation"/>        
    <Translation slot="Main" lang="en"/>
    <Testimonials slot="Testimonials" category="translation"/>
    <Footer slot="Footer" lang="en"/>   

</HomeLayout>
