---
import LegalLayout from '../layouts/LegalLayout.astro';
import Terms from '../components/Terms.astro';
import NavBar from '../components/NavBar.astro';
import Footer from '../components/Footer.astro';
const nav = {
    lang: "en",
    loc: "tc"
}
---

<LegalLayout>
<title slot="headerInfo">Terms and Conditions</title>
    <NavBar slot="NavBar" lang={nav.lang} location={nav.loc}/> 
    <Terms slot="Main" lang={nav.lang} />
    <Footer slot="Footer" lang={nav.lang}/>   

</LegalLayout>
