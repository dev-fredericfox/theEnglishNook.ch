---
import HomeLayout from '../layouts/HomeLayout.astro';
import MetaSEO from '../components/MetaSEO.astro';
import Consultation from '../components/Consultation.astro';
import NavBar from '../components/NavBar.astro';
import SideBar from '../components/SideBar.astro';
import Footer from '../components/Footer.astro';
---

<HomeLayout>
    <MetaSEO slot="MetaSEO" lang="en" location="consultation"/>
    <NavBar slot="NavBar" lang="en" location="consultation"/>
    <SideBar slot="SideBar" lang="en" location="consultation"/>        
    <Consultation slot="Main" lang="en" client:visible></Consultation>
    <Footer slot="Footer" lang="en"/>   

</HomeLayout>