---
import HomeLayout from '../layouts/HomeLayout.astro';
import MetaSEO from '../components/MetaSEO.astro';
import Contact from '../components/Contact.astro';
import ContactForm from '../components/ContactForm.vue';
import NavBar from '../components/NavBar.astro';
import SideBar from '../components/SideBar.astro';
import Footer from '../components/Footer.astro';
---

<HomeLayout>
    <MetaSEO slot="MetaSEO" lang="en" location="contact"/>
    <NavBar slot="NavBar" lang="en" location="contact"/>
    <SideBar slot="SideBar" lang="en" location="contact"/>        
    <Contact slot="Main" lang="en"/>
    <ContactForm slot="Main" lang="en" client:visible/>
    <Footer slot="Footer" lang="en"/>   

</HomeLayout>