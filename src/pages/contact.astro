---
import HomeLayout from '../layouts/HomeLayout.astro';
import Contact from '../components/Contact.astro';
import ContactForm from '../components/ContactForm.vue';
import NavBar from '../components/NavBar.astro';
import SideBar from '../components/SideBar.astro';
import Footer from '../components/Footer.astro';
---

<HomeLayout>
<title slot="headerInfo">Welcome</title>
    <NavBar slot="NavBar" lang="en" location="contact"/>
    <SideBar slot="SideBar" lang="en" location="contact"/>        
    <Contact slot="Main" lang="en"/>
    <ContactForm slot="Main" lang="en" client:visible><h1>FORM</h1></ContactForm>
    <Footer slot="Footer" lang="en"/>   

</HomeLayout>